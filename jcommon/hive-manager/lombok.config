# Lombok configuration file
# This file helps IDEs understand Lombok annotations better

# Enable annotation processing
lombok.addLombokGeneratedAnnotation = true

# Configure getter/setter generation
lombok.accessors.fluent = false
lombok.accessors.chain = false

# Log configuration
lombok.log.fieldName = log
lombok.log.fieldIsStatic = true

# Enable null check generation
lombok.nonNull.exceptionType = NullPointerException

# Copy annotations to generated methods
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Qualifier
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Value
lombok.copyableAnnotations += org.springframework.security.core.annotation.AuthenticationPrincipal 