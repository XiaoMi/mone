<!--        <table schema="error_record" tableName="error_record" domainObjectName="ErrorRecord">-->
<!--            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>-->
<!--        </table>-->


    </context>
</generatorConfiguration>