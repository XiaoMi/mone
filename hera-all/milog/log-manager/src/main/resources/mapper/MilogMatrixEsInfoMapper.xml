<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaomi.mone.log.manager.mapper.MilogMatrixEsInfoMapper">

    <select id="selectByCluster" resultType="com.xiaomi.mone.log.manager.model.pojo.MilogMatrixEsInfoDO">
        SELECT
            *
        FROM
            milog_matrix_esinfo
        WHERE
            cluster = #{cluster}
    </select>
</mapper>