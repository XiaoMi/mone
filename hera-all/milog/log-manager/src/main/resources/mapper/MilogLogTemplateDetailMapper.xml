<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaomi.mone.log.manager.mapper.MilogLogTemplateDetailMapper">

    <select id="getByTemplateId" resultType="com.xiaomi.mone.log.manager.model.pojo.MilogLogTemplateDetailDO">
        SELECT
            *
        FROM
            milog_log_template_detail
        WHERE
            template_id = #{templateId}
    </select>
</mapper>
