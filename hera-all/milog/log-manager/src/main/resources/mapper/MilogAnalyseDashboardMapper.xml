<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xiaomi.mone.log.manager.mapper.MilogAnalyseDashboardMapper">

    <select id="getByStoreId" resultType="com.xiaomi.mone.log.manager.model.pojo.MilogAnalyseDashboardDO">
        SELECT
            *
        FROM
            milog_analyse_dashboard
        WHERE
            store_id = #{storeId}
    </select>
</mapper>
